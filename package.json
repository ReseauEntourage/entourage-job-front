    "array-move": "^2.2.2",
    "axios": "^0.24.0",
    "circular-dependency-plugin": "^5.2.2",
    "dotenv": "^10.0.0",
    "google-maps-react": "^2.0.6",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lodash": "^4.17.21",
    "moment": "^2.24.0",
    "next": "^12.0.7",
    "next-with-less": "^2.0.2",
    "prop-types": "^15.7.2",
    "pusher-js": "^7.0.3",
    "react": "^17.0.2",
    "react-countup": "^4.3.3",
    "react-dom": "^17.0.2",
    "react-image-file-resizer": "^0.3.8",
    "react-modal": "^3.14.4",
    "react-select": "^3.0.8",
    "react-share": "^3.0.1",
