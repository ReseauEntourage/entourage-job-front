{
  "name": "linkedout-front",
  "version": "1.15.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "dev": "npx babel-node -r dotenv/config server-next.js",
    "start": "NODE_ENV=production node -r dotenv/config server-next.js",
    "lint": "eslint .",
    "format": "prettier --write './**/*.{js,jsx,md,json}' --config .prettierrc.json",
    "build": "next build",
    "uikit-install": "cd ./node_modules/uikit && npm i",
    "uikit-update": "cd ./node_modules/uikit/ && git pull && npm i",
    "uikit-compile": "cp -r frontend/assets/custom/ ./node_modules/uikit/custom/ && cd ./node_modules/uikit && npm run compile && cd ../../ && cp -r ./node_modules/uikit/dist/ ./public/static/dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ReseauEntourage/entourage-job.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "precommit": "NODE_ENV=production lint-staged",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,md,json,html}": [
      "prettier --write",
      "git add"
    ],
    "yarn.lock": [
      "git rm --cached"
    ]
  },
  "dependencies": {
    "@sentry/node": "^5.27.3",
    "@sentry/react": "^5.29.0",
    "@sentry/tracing": "^5.27.3",
